    "\n",
    "#define the class Bootstrap Result:\n",
    "class BootstrapResult:\n",
    "    def __init__(self, statistic, confidence_interval):\n",
    "        self.statistic = statistic\n",
    "        self.confidence_interval = confidence_interval\n",
    "\n",
    "#def the function bootstrap to make the data into 1D array\n",
    "def bootstrap(data, statistic_func, num_samples=1000, confidence_level=0.9, random_state=rng):\n",
    "    data = np.ravel(data)  # flatten the input data array\n",
    "    n = len(data) #store the flattened array in n\n",
    "    samples = random_state.choice(data, size=(num_samples, n), #generate samples with the same length n\n",
    "                                  replace=True) #and samples are drawn with replace\n",
    "    stats = np.apply_along_axis(statistic_func, axis=1, arr=samples) \n",
    "    #The apply_along_axis() function applies the statistic_func function to each row of the samples array (i.e., along axis=1) and returns a 1D array of statistics.\n",
    "    lower_percentile = (1 - confidence_level) / 2\n",
    "    upper_percentile = 1 - lower_percentile\n",
    "    lower_ci = np.percentile(stats, lower_percentile * 100)\n",
    "    upper_ci = np.percentile(stats, upper_percentile * 100)\n",
    "    return BootstrapResult(statistic_func(data), (lower_ci, upper_ci))\n",
    "\n",
    "std_confint = []\n",
    "for i, col in enumerate(df1.columns):\n",
    "#     print(df1[col])\n",
    "    std_sample = np.std(df1[col])  # calculate standard deviation of the column\n",
    "    res = bootstrap(df1[col], statistic_func=np.std, num_samples=1000, confidence_level=0.9, random_state=rng)\n",
    "    std_confint.append(res.confidence_interval)\n",
    "        \n",
    "std_confint"
   ]
