    "# Correct one!!!\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "from pathlib import Path\n",
    "from collections import defaultdict\n",
    "\n",
    "k = 1\n",
    "directory = \"Datasets/\"\n",
    "data = defaultdict(list)\n",
    "\n",
    "#create a template for dict\n",
    "columns = [\"avg_rss12\",\"var_rss12\",\"avg_rss13\", \"var_rss13\",\"avg_rss23\", \"var_rss23\"]\n",
    "parameters = [\"mean\",\"min\",\"max\",\"median\",\"qua1\",\"qua3\",\"std\"] \n",
    "\n",
    "for col in columns:\n",
    "    for p in parameters:\n",
    "        data[p+\"_\"+col] = []\n",
    "\n",
    "#iterate over files\n",
    "for file in Path(directory).glob(\"**/*.csv\"):\n",
    "    try:\n",
    "        df = pd.read_csv(file, skiprows=4)\n",
    "        df = df.drop('# Columns: time', axis=1)\n",
    "#         print(df)\n",
    "        for col in df.columns[0:]:\n",
    "            a = pd.DataFrame(df[col])\n",
    "#             print(\"Column:\", col)\n",
    "#             print(\"Mean:\", np.mean(a)[col])\n",
    "#             print(\"Min:\", np.min(a)[col])\n",
    "            #get 7 kinds of parameters for each column\n",
    "            data[\"mean\" + '_' + col].append(np.mean(a, axis = 0)[col])\n",
    "            data[\"min\" + '_' + col].append(np.min(a, axis = 0)[col])\n",
    "            data[\"max\" + '_' + col].append(np.max(a, axis = 0)[col])\n",
    "            data[\"median\" + '_' + col].append(np.median(a))\n",
    "            data[\"qua1\" + '_' + col].append(np.quantile(a, 0.25))\n",
    "            data[\"qua3\" + '_' + col].append(np.quantile(a, 0.75))\n",
    "            data[\"std\" + '_' + col].append(np.std(a)[col])\n",
    "    except (pd.errors.EmptyDataError, pd.errors.ParserError, KeyError):\n",
    "        #skip files that cannot be read or do not contain the expected columns\n",
    "        continue\n",
    "        \n",
    "df1 = pd.DataFrame.from_dict(data)\n",
    "df1"
   ]
